{
  "buildCommand": "0x1 build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "framework": null,
  "routes": [
    {
      "src": "/0x1/(.*)",
      "dest": "/0x1/$1"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/app-bundle.js",
      "dest": "/app-bundle.js"
    },
    {
      "src": "/app/(.*)\\.js",
      "dest": "/app/$1.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/components/(.*)\\.js",
      "dest": "/components/$1.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "dest": "/service-worker.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
