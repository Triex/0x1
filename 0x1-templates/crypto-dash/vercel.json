{
  "buildCommand": "0x1 build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "framework": null,
  "headers": [
    {
      "source": "/0x1/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/0x1/(.*)",
      "destination": "/0x1/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
