{
  "buildCommand": "0x1 build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "framework": null,
  "headers": [
    {
      "source": "/app/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/components/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/0x1/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/node_modules/0x1",
      "dest": "/0x1/index.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/0x1/(.*)",
      "dest": "/0x1/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/app-bundle.js",
      "dest": "/app-bundle.js"
    },
    {
      "src": "/app/(.*)\\.js",
      "dest": "/app/$1.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/components/(.*)\\.js",
      "dest": "/components/$1.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "dest": "/service-worker.js"
    },
    {
      "src": "^/(?!api/).*",
      "dest": "/index.html"
    }
  ]
} 